@model  List<EcommerceSistemaVsShopify.Models.Usuarios_asp_core>
@{
    ViewData["Title"] = "Pagina Principal";
}

@*<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn abt <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Ce</a>.</p>
